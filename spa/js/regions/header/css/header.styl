// begin b-navbar
.b-navbar {
    background: $b-navbar-bg;
}
.b-navbar__container {
    display: flex;
    justify-content: space-between;
    align-items: center;

    position: relative;
    width: $site-width;
    margin: 0 auto;
    padding: 4px 0;
}
.b-navbar__promo {
    font-size: $b-navbar-font-size;
    color: $b-navbar-color;
    line-height: $b-navbar-line-height;
}
.b-navbar-menu {
    display: flex;
}
.b-navbar-menu__item {
    margin-left: 15px;
}
.b-navbar-menu__link {
    display: block;
    color: $b-navbar-color;
    font-size: $b-navbar-font-size;
    line-height: $b-navbar-line-height;
}
// end b-navbar
