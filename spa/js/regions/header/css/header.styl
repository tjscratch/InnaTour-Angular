// begin header-user
.header-user {
    margin-left: 30px;
}
.header-user__items {
    display: flex;
    align-items: center;
}
.header-user__item {
    padding: 3px 10px;
    border-left: 1px solid #303C47;
    white-space: nowrap;
    cursor: pointer;
    display flex
    align-items center
    margin-top: 5px;

    &:first-child {
        padding-left: 0;
        border-left: none;
    }
    &:last-child {
        padding-right: 0;
        padding-top: 2px;
    }
}
.header-user__item-icon {
    display: inline-block;
    margin-right: 6px;
    color: $header-user__item-icon-color;
    font-size: 1.1rem;
}
.header-user__item-text {
    display: inline-block;
    font-size: 18px;
    color: #EFF2F5;
    font-family: Roboto;
}
.header-user__item-text_black, .header-info_black {
    color #303C47 !important
}
// end header-user
