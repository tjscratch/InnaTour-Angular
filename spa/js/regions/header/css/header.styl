.header-banner-grey{
    background #DDE3E9
}
.header__container {
}
.header-nav {
    font-size: 15px;
    font-family: $ProximaNovaMedium;
    color: #778695;
    z-index: 300;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    margin-bottom: 6px;
}
.header-nav__item {
    flex-basis: 160px;
    &:first-child{
        a{
            border-radius 3px 0 0 3px
        }
    }
    &:last-child{
        a{
            border-radius 0 3px 3px 0
        }
    }

    position: relative;
    display: inline-block;
    //margin: 0 2px 0 0;
    margin: 1px 1px 0 0;
}
.header-nav__item-promo {
    position: absolute;
    left: -16px;
    top: -16px;
    width: 32px;
    height: 32px;
    line-height: 32px;
    border-radius: 16px;
    background-color: $green;
    color: $white;
    font-weight: 600;
    font-size: 13px;
    text-align: center;
    text-shadow: 0 1px 0 rgba(0, 0, 0, .24);
}
.header-nav__item-link {
    display: block;
    padding: 4px 16px;
    //background-color: $header-nav__item-link-bg;
    //color: $header-white;
    font-size: 15px;
    line-height: 32px;
    font-weight: normal;
    background-color #DDE3E9
    color #778695
    //border-radius: 3px 3px 3px 3px;
    text-align center;

    &:hover {
        color $header-white
        text-decoration none
    }
}
.header-nav__item-link-active {
    //background-color: $header-nav__item-link-active-bg;
    background-color #ffffff
}
// begin toggle-mobile
.toggle-mobile {
    position fixed
    left 0
    right 0
    top 0
    z-index 1000
    //height 40px
    background rgba(255, 255, 255, .9)
    text-align center
    font-size 1em
}
.toggle-mobile__container {
    position relative
    max-width $site-width
    margin 0 auto
}
.toggle-mobile__link {
    display inline-block
    padding 2px 5px
    color #0054b9
}
.toggle-mobile__close {
    position absolute
    right 12px
    top 5px
    font-size 16px
    cursor pointer

    .fa {
        margin-left 3px
        font-size 18px
    }
}
.balloonIsMobile {
    width 800px
    padding-top 40px
    padding-bottom 40px

    .balloon-close {
        right 8px
        font-size 35px
    }
    .balloon-title {
        margin 0 0 20px
        font-size 37px
    }
    .balloon-info {
        font-size 40px

        .toggle-mobile__link {
            display inline-block
            padding 0
            color #0054b9
        }
    }
}
// end toggle-mobile
