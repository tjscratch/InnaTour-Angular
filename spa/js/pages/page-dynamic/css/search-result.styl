.results-container {
  @extend .main__center;
  //padding $full_padding 0 0
  font-family $ProximaNovaLight
  position relative
  color #333
  padding-top 333px
  background-color #EFF2F5
  clearfix()


  &.results-container_top {
    //padding 10px 0 0
    padding 0px
  }

  &.results-container__short {
    //padding-top $short_padding
    padding-top 2px
  }

}

.res-aside-avia
    width: 235px
    float: right
    background-color: #fece07

.res-aside-avia__img
    width: 235px;
    background-image: url('/spa/img/banners/banner-avia.png');

    cursor: pointer;

    &.two-way
        height: 180px;
        background-position: 0px 0px;
        background-size : 479px auto;

    &.one-way
        height: 130px;
        background-position: -255px -50px;
        background-size : 479px auto;