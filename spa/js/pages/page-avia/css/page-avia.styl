.results__body_mod_avia {
  //padding-top: $padding_over_search;
  padding-top 140px
}
.banner-sale-dp {
  position absolute
  top 16px
  right 16px
  width 168px
  height 136px
  padding-top 26px
  color #313C48
  background-color #FAC700
  border-radius 3px
  text-align center
  box-sizing border-box
  cursor default
}
.banner-sale-dp__value {
  font-size 36px
}
.banner-sale-dp__desc {
  font-size 14px
}
.banner-sale-dp__btn {
  margin 9px auto 0px
  width 136px
  height 32px
  line-height 32px
  font-size 14px
  background-color #FCD53D
}
.about-fly {
  width 100%
  color #313C48 100%
  background-color #EFF2F5
  border 1px solid #DDE3E9
  border-top none
  border-radius 0px 0px 3px 3px
  margin-top -10px
  margin-bottom 8px
  box-sizing border-box
}
.about-fly__title {
  padding-top 15px
  text-align center
  font-size 16px
  font-weight bold
}
.about-fly__way {
  padding 16px 16px 0px
  text-align left
  font-size 16px
  font-weight bold
}
.about-fly__etap {
  padding 4px 16px
  font-size 15px
}
.about-fly__etap-wrap {
  border-bottom 1px dotted #9CA6B0
}
.about-fly-etap__row {
  padding 4px 0px
}
.about-fly-etap__row_logo-time {
  display flex
  align-items center
  justify-content space-between
}
.about-fly-etap__row-group {
  display flex
  align-items center
}
.about-fly-etap__row-icon {
  height 24px
  width 24px
  margin-right 8px
}
.about-fly-etap__row-item {
  margin-right 32px
}
.about-fly__gray-text {
  color #778695
}
.about-fly-etap__des {
  display flex
  align-items center
  justify-content flex-start
}
.about-fly-etap__des-item {
  margin-right 32px
}
.about-fly-etap__des-aircraft-name {
  margin-left 32px
  font-size 12px
  @extend .about-fly__gray-text
}
.about-fly-etap__transfer {
  display flex
  align-items center
  justify-content space-between
  padding 12px 0px
  background-color #DDE3E9
  font-size 15px
}
.about-fly-etap__transfer-group {
  display flex
  align-items center
  justify-content space-between
  width 706px
}
.about-fly-etap__transfer-item {
  display flex
  align-items center
}
.about-fly-etap__transfer-item_alert {
  width 275px
  color #DB4437
}