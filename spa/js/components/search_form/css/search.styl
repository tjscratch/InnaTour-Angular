.search-pref-block
    position absolute
    z-index 10
    left 0
    top 49px
    margin 0 0 0 -1px
    padding 0
    white-space nowrap
    background rgba(0, 0, 0, .4)
    box-shadow 0 4px 8px 1px rgba(0, 0, 0, .4)
.search-pref
    display inline-block
    vertical-align top
    font 14px $ProximaNova
    line-height normal
    letter-spacing normal
    color #fff
    margin 0 0 0 1px
    padding 5px 12px
    cursor pointer
    background #393939
    background: linear-gradient(to top, #4c4c4c 0%, #393939 50%)
    position relative
.search-pref-comfort
    padding-right 30px
    background #89c13a
    min-width 57px
    background url(/spa/img/i.png) no-repeat right 12px top -233px, linear-gradient(to top, #95c74d 0%, #89c13a 50%)
.search-pref.current
    color #89c13a
.search-pref-select
    white-space normal
    position absolute
    left 0
    right 0
    top 27px
    margin 0
    padding 5px 12px
    list-style none
    background #89c13a
    box-shadow 0 7px 8px 1px rgba(0, 0, 0, .3)
.search-people-baloon
    position absolute
    left 0
    padding 12px 12px 0
    min-width 215px
    background #fff
    box-shadow 0 5px 10px rgba(50, 50, 50, 0.25)
    z-index 10
    margin-top 1px
.search-people-baloon-title
    font-size 14px
    line-height 1
    padding 0 0 14px
.search-people-baloon-item
    overflow hidden
    padding 0 0 14px
    font-size 14px
.search-people-baloon-field
    float left
    position relative
.search-people-baloon-field input
    display block
    padding 3px
    margin 0 31px
    font 14px $ProximaNova
    text-align center
    outline none
    border 1px solid #ccd2d4
    width 30px
    height 30px
    box-sizing border-box
    border-radius 3px
.search-people-baloon-minus
.search-people-baloon-plus
    color #878787
    position absolute
    top 0
    bottom 0
    width 30px
    border-radius 3px
    text-align center
    line-height 30px
    cursor pointer
    height 30px
    font-size 20px
    user-select none
    font-weight bold

    &:hover
        background #e6e7e9
&.hover
    background #89c13a
    color #fff
    &:hover
        background #89c13a
.search-people-baloon-null input
    color #ccc
.search-people-baloon-minus
    left 0
.search-people-baloon-plus
    right 0
.search-people-type
    float left
    width 110px
    margin 0 10px 0 0
    font-size 14px
    color #999
    letter-spacing -.025em
.search-people-type b
    font-weight normal
    display block
    color #333
.search-autocomplete
    width 340px
    background #fff
    overflow auto
    overflow-x none
    overflow-y auto
    max-height 360px
    position absolute
    z-index 50
    font-size 16px
    letter-spacing -.025em
    padding 5px 0
    box-shadow 0 5px 10px rgba(50, 50, 50, 0.25)
    margin-top 49px

    > li:last-child
        > ul:after
            border-bottom none
.search-autocomplete-block
    padding 6px 8px 6px 15px
    display block
    font-size 15px
    color #333
    clearfix()
    cursor pointer

    &:hover
        background #e6e7e9

    &.selected
        background #e6e7e9
.search-autocomplete-city
    float left
    letter-spacing -.025em
.search-autocomplete-city em
    font-style normal
    font-size 14px
    color #878787
.search-autocomplete-city b
    color #878787
    font-weight normal
.search-autocomplete-abbr
    float right
    color #878787
    font 14px $ProximaNovaLight
    text-transform uppercase
.search-autocomplete li li .search-autocomplete-block
    padding-left 53px
.search-autocomplete ul
    position relative
    margin 0 0 2px
.search-autocomplete ul:after
    content ''
    display block
    border-bottom 1px solid #c0c0c2
    position absolute
    left 0
    right 0
    bottom 0
.ng-isolate-scope .search-pref-block
    top 51px
.search-form-dialog-row {
    .child-years-select-container {
        margin-right: 3px;

        &:last-child {
            margin-right: 0px;
        }
    }

