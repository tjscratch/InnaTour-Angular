@media all and (max-width: 974px) {
    .dp-search-reserve {
        flex-basis: 50%;
        display: flex;
    }
    .b-wrapp_type_inline {
        display: flex;
        flex: 1;
        flex-basis: 46%;
    }
    .search__city {
        flex-basis: 49%;
        margin-left: 1px !important;
    }

    .search-date-block {
        margin-top: 1px;
        width: 100%;
    }
    .search-form-humans {
        flex: 1;
        margin-top: 1px;
    }
    .search-people-block{
        margin-top: 1px;
        flex-basis: 49%;
        flex: 1;
        width: 40%;
    }
    .search-form__submit{
        margin: 1px 1px 2px 1px;
        border-radius: 0;
        flex-basis: 100%;
    }
    .search-field, .search-date{
        border-radius: 0 !important;
    }
    .b-search-form .b-location_selector:nth-child(2) .search-field {
        border-radius: 0;
    }
    .search-device{
        margin-right: 1px;
    }
    .human-class{
        display: flex;
        flex: 1;
        flex-wrap: wrap;
        flex-basis: 46%;
    }
    .header-info__container{
        width 98%;
    }
}
@media (max-width: 768px) {
    .b-search-form_padding-16px {
        padding: 67px 0px 16px 0;
    }
    .search-autocomplete {
        right: 42px
    }
}
@media (max-width: 375px) {
    .b-wrapp_type_inline{
        flex-basis: 99%;
    }
    .search__city {
        margin-top: 1px;
        margin-right: 1px;
    }
    .search-form-humans {
        margin-left: 1px;
    }
    .offer-extra-large .offer-title {
        font-size: 25px;
    }
}
@media all and (max-width: 320px) {
    .search__city {
        flex-basis: 100%;
        margin-top: 1px;
        margin-right: 1px;
    }
    .b-wrapp_type_inline{
        flex-basis: 100%;
    }
}