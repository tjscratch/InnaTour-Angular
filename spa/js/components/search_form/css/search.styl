    font-family: 'Roboto-Regular', Arial, sans-serif
.search-field::-webkit-input-placeholder
    color #878787
    opacity 1
    //font-family $ProximaNovaLight
.search-field:-ms-input-placeholder
    color #878787
    opacity 1
    //font-family $ProximaNovaLight
.search-date::-webkit-input-placeholder
    color #878787
    opacity 1
    //font-family $ProximaNovaLight
.search-date:-ms-input-placeholder
    color #878787
    opacity 1
    //font-family $ProximaNovaLight
.search-people-baloon-field input::-webkit-input-placeholder
    color #878787
    opacity 1
    //font-family $ProximaNovaLight
.search-people-baloon-field input:-ms-input-placeholder
    display none
.search-field {
    padding 0 0px 0 10px
}
.no-outline-on-focus {
    &:focus {
        outline: none;
    }
}
.search-date-block
    //width 118px !important
    position: relative;
    margin-right: 1px;
    width: 121px;

    &:focus {
        outline: none;
    }

    &__date {
        background: #fff;
        padding-left: 5px;
        //height: 36px;
        line-height: 38px;
        position: absolute;
        top: 1px;
        left: 1px;
        bottom: 1px;
        right: 0;
        align-items: center;
        display: flex;
        padding-top: 5px;

        &_display_placeholder {
            //color: #666;
            color #778695
        }
    }

    &:after {
        //@extend .icon-sprite-calendar;
        @extend .icon-calendar;
        content: '';
        position absolute;
        right: 5px;
        top: 13px;
        cursor: pointer;
        //margin-top: -8px;
    }
.search-date
    padding 0 28px 0 10px
.search-button-block
    border-left 1px solid #1c1c1c
    border-right 0
    background #89c13a
    color #fff
    font-family $ProximaNova
    text-align center
    position relative
    overflow hidden
    height 48px
    line-height 48px
    width 117px
    font-weight bold
    transition background-color 0.3s linear
.search-form__submit {
    border: 0;
    /* position: absolute; */
    /* overflow: hidden; */
    height: 48px;
    width: 99px;
    /* margin: 0; */
    padding: 14px 10px;
    transition: background-color 0.3s linear;
    background-color: #89c13a;
    cursor: pointer;
    font-size: 16px;
    font-family: Roboto-Medium;
    color: #fff;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    text-align: center;
    border-radius: 0px 3px 3px 0px;
    /* right: 0px; */
    //margin-left: 1px;

    .search-button__icon {
        position: absolute;
        transform: translate(-50%, -50%) scale(0.6);
        top: 50%;
        left: 50%;
        z-index: 1;
    }
}
.search-button-block:hover
.search-button-block_small:hover
    background-color #95cd47
.search-button-block_small input
.search-button-block input
    position absolute
    display block
    left 0
    top 0
    bottom 0
    right 0
    width 100%
    cursor pointer
    border 0
    opacity 0
    margin 0
    padding 0
