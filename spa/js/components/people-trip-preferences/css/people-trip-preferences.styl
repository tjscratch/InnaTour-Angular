.list-childrens {
  width 100%
  height 144px
  overflow-y auto
  border-radius 3px
  border 1px solid #C9D2DB
}
.list-childrens__item {
  width 100%
  height 25px
  line-height 25px
  padding-left 10px
  box-sizing border-box
  cursor pointer
}