.minusAdult, .plusAdult {
  width 33px
  height 32px
  display inline-block
  float: left
  box-sizing border-box
  cursor pointer
}
.minusAdult {
  border-radius 3px 0px 0px 3px
  border 1px solid #C9D2DB
}
.plusAdult {
  border-radius 0px 3px 3px 0px
  border 1px solid #C9D2DB
}
.horizontal-line {
  position absolute
  width 14px
  height 2px
  margin-left 8px
  margin-top 13px
  background-color #313C48
}
.vertical-line {
  position absolute
  height 14px
  width 2px
  margin-left 14px
  margin-top 7px
  background-color #313C48
}
.countAdult {
  line-height 32px
  height 32px
  width 94px
  display inline-block
  float: left
  text-align center
  box-sizing border-box
  border-top 1px solid #C9D2DB
  border-bottom 1px solid #C9D2DB
  user-select none
}
.search-form-dialog-row {
  width 160px
  margin auto
  margin-bottom 10px
}
.add-children, .childrens {
  padding-left 10px
  height 32px
  width 100%
  line-height 32px
  border-radius 3px
  border 1px solid #C9D2DB
  position relative
  box-sizing border-box
  cursor pointer
}
.childrens {
  margin-bottom 5px
}
.delete-children {
  width 20px
  height 20px
  position absolute
  top 5px
  right 5px
}
.delete-children__item-one {
  position absolute
  height 2px
  width 20px
  margin-top 8px
  transform rotate(45deg)
  background-color #313C48
}
.delete-children__item-two {
  position absolute
  height 2px
  width 20px
  margin-top 8px
  transform rotate(-45deg)
  background-color #313C48
}
