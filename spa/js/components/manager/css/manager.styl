.b-manager{
    padding 20px 0
    background #f6f6f6
}
.b-manager__container{
    position relative
    width 960px
    height 350px
    margin 0 auto
}
.b-manager__iframe{
    position relative
    z-index 1

    width 100%
    height 100%
    border none
    vertical-align top
}
.manager-full-view{
    position absolute
    right 10px
    top 10px
    z-index 1001

    width 14px
    height 15px
    background url("/spa/img/manager-full-view.png") no-repeat 0 0
    cursor pointer
}
.b-manager__container-open{
    position fixed
    top 0
    bottom 0
    left 0
    right 0
    z-index 16000003

    width 100%
    height 100%
    padding 0
}
