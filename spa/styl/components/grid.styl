.container {
	margin-right: auto;
	margin-left: auto;
	padding-left: 15px;
	padding-right: 15px;
}
.container-fluid {
	margin-right: auto;
	margin-left: auto;
	padding-left: 15px;
	padding-right: 15px;
}


.row {
	box-sizing : border-box;

	&:after {
		content: '';
		display : block;
		clear : both;
	}
	/*margin-left: -15px;
	margin-right: -15px;*/
}

.row-fluid {
  display: table;
  box-sizing: border-box;
  width: 100%;
}

.row-fluid__cell {
  display: table-cell !important;
  vertical-align: top;
  box-sizing: border-box;
  position: relative;
  float : none !important;

  &-v-middle {
    vertical-align : middle;
  }
  &-v-bottom {
    vertical-align : bottom;
  }
}

.col-no-padding {
	padding : 0 !important;
}

.col-no-wrap {
    white-space: nowrap;
}



/* custom grid width */
/*$col-cell-460 = 460px;
$col-cell-480 = 480px;
$col-cell-390 = 390px;
$col-cell-next-500 = 534px;
$col-cell-next-300 = 334px;*/

.col-cell-460 {
	width : $col-cell-460;
}

.col-cell-480 {
	width : $col-cell-480;
}

.col-cell-390 {
	width: $col-cell-390;
}

.col-cell-530 {
	width: $col-cell-next-500;
}

.col-cell-330 {
	width: $col-cell-next-300;
}

.col-cell-230 {
	width: $col-cell-next-200;
}
/* \ custom grid width */




colsize = (8.33333333% 16.66666667% 25% 33.33333333% 41.66666667% 50% 58.33333333% 66.66666667% 75% 83.33333333% 91.66666667% 100%);

// созаем на лету переменные с размерами блоков
for prop, key in colsize {
  //col_xs_size_{key} = prop;

  .col-sm-{key+1},
  .col-md-{key+1},
  .col-lg-{key+1},
  .col-xs-{key+1} {
    position: relative;
    min-height: 1px;
    padding-left: 15px;
    padding-right: 15px;
  }
  .col-xs-pull-{key+1} {
    position: relative;
  }
  .col-xs-push-{key+1} {
    position: relative;
  }
  .col-xs-{key+1} {
    box-sizing : border-box;
    float: left;
    width : prop;
  }
}


