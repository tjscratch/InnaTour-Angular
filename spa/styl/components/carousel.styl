.b-carousel {
	overflow: hidden;
	width: 100%;


	&__prev {

	}

	&__next {

	}

	&__holder {
		overflow: hidden;
	}

	&__slider {
		position: relative;
		overflow : hidden;
		transition: transform .2s linear

		&:after {
			content: '';
			display : block;
			clear : both;
		}
	}

	&_item {
		float: left;
		text-align : center;
		position : relative;
	}
}
