.datepickerSelected-start a
.datepickerSelected-end a
    background #89c13a
    color #fff !important
    position relative

.datepickerSelected-start a span
.datepickerSelected-end a span
    position absolute
    font 14px $ProximaNova
    right 1px
    top 1px
    letter-spacing -.025em
    line-height 1

.datepickerSelected-start a:before
.datepickerSelected-end a:before
    display block
    content ''
    width 16px
    height 15px
    position absolute
    left 0
    bottom 0
    background url(/spa/img/i.png) no-repeat

.datepickerSelected-start a:before
    background-position 0 -2057px

.datepickerSelected-end a:before
    background-position 0 -2088px

.datepickerSelected-start.datepickerSelected-end a:before
    display block
    content ''
    width 30px
    height 13px
    position absolute
    left 0
    bottom 0
    background url(/spa/img/1day.png) no-repeat

.datepicker-body
    position absolute
    left -10px
    top 38px
    z-index 900

.datepickerDisabled a
    color #ACACAC !important

    &:hover
        background none !important
        color #ACACAC !important

.datepickerNotInMonth a
    opacity 0

.datepickerDisabled.datepickerSaturday a
.datepickerDisabled.datepickerSunday a
    color #FFA3A3 !important

.datepickerSpace div
    width 38px