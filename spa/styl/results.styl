.ui-tooltip
    background #494949
    font-size 14px
    letter-spacing -.025em

.ui-tooltip:before
    border-color: transparent transparent #494949 transparent

.rl-title-best
    font-size 10px
    text-transform uppercase
    color #fff
    line-height 1
    padding 4px 15px 5px

.rl-title-cheapest
    background #eba600

.rl-title-recommended
    background #4ca6cb

// Filters
.filters__body
    min-height 41px

.filters__container
    background #fff
    margin 0 auto
    box-shadow 0 3px 5px rgba(0,0,0,.3)
    width 960px
    clearfix()

    > li
        float left
        position relative
        margin 0 26px 0 0

.filters__link
    display block
    padding 14px 35px 13px 12px
    color #89c13a
    font-size 14px
    line-height 1
    transition all .3s
    position relative

    &:before
        content ''
        display block
        position absolute
        right 17px
        top 19px
        width 12px
        height 12px
        background url("../img/i.png") no-repeat right -195px

    &:hover
        background-color #e6e7e9
        text-decoration none
        color #1a1a1a

.filters__baloon
    position absolute
    left 0
    background #fff
    z-index 100
    width 320px
    box-shadow 0 3px 5px rgba(0,0,0,.3)
    padding 3px 0

.filters__baloon_240
    width 240px

.filters__container .filters__sort
    float right
    margin 0

    a
        color #666

.filter__range-container
    clearfix()
    padding 14px 12px 21px

    .ra-header-link
        margin-top 5px

.filter__range
    float left
    font-size 14px
    color #1a1a1a

    input
        border 1px solid #c0c0c2
        border-radius 3px
        width 90px
        box-sizing border-box
        margin 0 5px
        padding 5px 7px

        &:focus
            border-color #89c13a

.filter__range-trackbar
    position relative
    margin 0 12px 30px

.filters__baloon-radio
    margin 0 12px 5px
    clearfix()

    input
        display none

        &:checked + .ra-radio
            background #89c13a
            color #fff

    li
        float left
        border-left 1px solid #fff

        &:first-child
            border 0

            .ra-radio
                border-radius 3px 0 0 3px

        &:last-child .ra-radio
            border-radius 0 3px 3px 0

    label
        display block
        width 107px

.filters__baloon-days
    margin 0 12px
    clearfix()

    .ra-fly-type-list
        float left
        overflow hidden
        width 108px

.filters__container_fixed
    position fixed
    top 150px
    left 0
    z-index 500
    width 100%


.ra-radio
    text-align center
    font-size 14px
    background #e6e7e9
    padding 6px 0
    cursor pointer
    display block


.rl-left
    float left
    width 515px

.rl-item-separator
    position absolute
    right 178px
    top 0
    bottom 0
    display block
    width 1px
    background #ccc

    &:before
        content ''
        display block
        position absolute
        left -5px
        top -3px
        background url(../img/corner-top.png) no-repeat
        width 10px
        height 8px

    &:after
        content ''
        display block
        position absolute
        left -5px
        bottom -5px
        background url(../img/corner-bot.png) no-repeat
        width 10px
        height 10px

.rl-aside
    float right
    width 160px

.rl-rec
    position absolute
    right 10px
    top 12px
    width 160px
    font-size 12px

    &:before
        content ''
        display block
        background url(../img/logo-small.png) no-repeat
        width 49px
        height 20px
        position absolute
        right 0
        top -2px

.ra-tags-container
    margin 0 0 10px

.ra-tags-title
    font-size 14px
    color #333
    font-weight normal
    padding 12px 0 20px

.ra-tags-list
    list-style none
    margin 0 0 5px

    li
        background #fff
        padding 6px 26px 6px 11px
        position relative
        display inline-block
        font-size 14px
        box-shadow 0 1px 4px rgba(0,0,0,.25)
        margin 0 1px 5px 0

        a
            display block
            position absolute
            width 9px
            height 9px
            padding 5px
            right 2px
            top 5px
            background url(../img/remove.png) no-repeat center center

.ra-tags-reset
    font-size 12px
    color #1a1a1a
    text-decoration underline

    &:hover
        text-decoration none

.ra-best-block
    background #edc63f
    border-radius 4px
    overflow hidden
    position relative
    box-shadow 1px 1px 4px rgba(0,0,0,.1)
    padding 16px 10px 54px
    text-align center
    margin 0 0 10px

.ra-best-select
    font-size 15px
    color #fff
    font-weight bold
    text-decoration none
    text-transform uppercase
    background #4d4d4d
    display block
    position absolute
    left 0
    right 0
    bottom 0
    padding 10px

.ra-best-descr
    font-size 14px
    line-height 20px
    color #4d4d4d

.ra-best-descr-info
    display block
    font-size 20px
    text-decoration line-through

.ra-best-price
    color #fff
    font-size 36px
