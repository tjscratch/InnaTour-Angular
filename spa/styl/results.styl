    position relative
    z-index 1

.ra-block
    border-bottom 1px solid #dedede

.ra-header
    clearfix()
    padding 12px 14px 6px

.ra-title
    float left
    font-weight bold
    font-size 14px

.ra-header-link
    font-size 12px
    color #1a1a1a
    float right
    margin-top 3px
