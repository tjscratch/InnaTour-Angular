.ra-fly-type-list
    padding 5px 14px 0 0
    color #1a1a1a
    margin 0 -14px 0 0
    max-height 280px
    overflow-y: auto
    position relative

.ra-fly-type-list-container
    position relative

    &:before
        content ''
        display block
        position absolute
        left 0
        right 0
        top 0
        height 10px
        background linear-gradient(to bottom, rgba(255, 255, 255, 1) 0, rgba(255, 255, 255, 0) 100%)
        z-index 5

    &:after
        content ''
        display block
        position absolute
        left 0
        right 0
        bottom 0
        height 10px
        background linear-gradient(to top, rgba(255, 255, 255, 1) 0, rgba(255, 255, 255, 0) 100%)
        z-index 5

.ra-fly-type-list li
    clearfix()
    font-size 14px
    padding 6px 0
    //position relative

.ra-fly-type-list input
    display none

.ra-fly-type
    float left
    max-width 200px

.ra-checkbox
    width 15px
    height 15px
    background #fff
    border 2px solid #89c13a
    float left
    border-radius 3px
    margin -2px 10px 0 0

.ra-fly-type-list input:checked + .ra-checkbox
    background url(/spa/img/i.png) no-repeat 2px -590px #89c13a

.ra-fly-price
    float right
    color #666

.ra-amount-time
    font-size 13px
    letter-spacing -.025em
    padding 0 0 18px

.ra-amount-price
    text-align center
    font-size 13px
