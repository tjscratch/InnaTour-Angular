        vertical-align middle
        margin 0 5px 0 0

.rl-airline-text
    position relative
    top 2px

.rl-footer
    clearfix()
    font-size 14px
    position relative
    padding 7px 0 0

.rl-more-info
    float right
    padding 15px 0 9px
    color #000
    text-decoration underline

.rl-more-info:hover
    text-decoration none

.rl-tickets-number
    float left
    padding 15px 14px 9px
    color #dd3e14

.rl-class
    position absolute
