    text-align center
    font-size 12px
    color #4d4d4d
    padding 0 0 0 10px
    line-height 18px

.rl-travel-time:before
    content ''
    display block
    position absolute
    border-bottom 1px solid #666
    top 18px
    left 0
    right 0

.rl-travel-time:after
.rlf-fly-arrow:after
    content ''
    display block
    width 10px
    height 5px
    background url(../img/i.png) no-repeat 0 -684px
