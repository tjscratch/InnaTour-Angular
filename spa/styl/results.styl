.results-body
    padding 0 0 10px

.results-container
    font-family $ProximaNova
    color #333
    width 960px
    margin 0 auto
    clearfix()
    position relative
    padding-top 25px

.results-container_top
    padding 10px 0 0

.results-aside
    width 246px
    float right
    background #fff

.results-aside_mod
    background none
    width 230px

.ra-block
    border-bottom 1px solid #dedede

.ra-header
    clearfix()
    padding 12px 14px 6px

.ra-title
    float left
    font-weight bold
    font-size 14px

.ra-header-link
    font-size 12px
    color #1a1a1a
    float right
    margin-top 3px
    text-decoration underline

    &:hover
        text-decoration none

.ra-content
    padding 0 14px

.ra-trackbar
    margin-top 6px !important

.ra-fly-type-list
    padding 5px 14px 0 0
    color #1a1a1a
    margin 0 -14px 0 0
    max-height 280px
    overflow auto
    overflow-x none
    overflow-y scroll
    position relative

.ra-fly-type-list-container
    position relative

    &:before
        content ''
        display block
        position absolute
        left 0
        right 0
        top 0
        height 10px
        background linear-gradient(to bottom, rgba(255, 255, 255, 1) 0,rgba(255, 255, 255, 0) 100%)
        z-index 5

    &:after
        content ''
        display block
        position absolute
        left 0
        right 0
        bottom 0
        height 10px
        background linear-gradient(to top, rgba(255, 255, 255, 1) 0,rgba(255, 255, 255, 0) 100%)
        z-index 5

.ra-fly-type-list li
    clearfix()
    font-size 14px
    padding 6px 0
    position relative

.ra-fly-type-list input
    display none

.ra-fly-type
    float left

.ra-checkbox
    width 15px
    height 15px
    background #fff
    border 2px solid #89c13a
    float left
    border-radius 3px
    margin -2px 10px 0 0

.ra-fly-type-list input:checked + .ra-checkbox
    background url(../img/i.png) no-repeat 2px -590px #89c13a

.ra-fly-price
    float right
    color #666

.ra-amount-time
    font-size 13px
    letter-spacing -.025em
    padding 0 0 18px

.ra-amount-price
    text-align center
    font-size 13px
    letter-spacing -.025em
    padding 0 0 14px
    margin -15px 0 0

.ui-slider
    height 4px
    background #d0d0d0
    margin 0 0 22px

.ui-slider .ui-slider-handle
    background url(../img/i.png) no-repeat 0 -639px
    width 15px !important
    height 22px !important
    top -8px
    transform rotate(180deg)
    outline none

.ui-slider-horizontal .ui-slider-handle
    margin-left -7px

.ui-slider-range
    background #89c13a !important

.results-list
    float left
    width 704px

.results-list_720
    width 720px

.rl-header
    background #fff
    clearfix()
    font-size 14px
    border-bottom 1px solid #c8c8c8
    padding 0 0 0 12px
    margin 0 0 9px
    letter-spacing -.025em

.rl-flights-number
    float left
    font-weight normal
    padding 12px 0

.rl-filter
    float right
    position relative

.rl-filter-current
    cursor pointer
    background url(../img/i.png) no-repeat right 12px top -176px
    padding 12px 34px 12px 12px

.rl-filter-menu
    position absolute
    right 0
    background #fff
    list-style none
    box-shadow: 0 8px 8px rgba(0,0,0,.4)
    padding 0 0 5px
    z-index 1
	
.rl-filter-menu li
    text-align right
    padding 6px 34px 6px 12px
    min-width 140px
    cursor pointer

.rl-filter-menu li:hover
    background #eee

.rl-item
    background #fff
    margin 0 0 10px
    border-radius 4px
    box-shadow 0 2px 3px rgba(0,0,0,.8)
    clearfix()
    position relative

.rl-item-header
    clearfix()
    padding 9px 14px 2px
    display block

.rl-price
    font-weight bold
    text-align center
    padding 68px 0 0
    display block

    .rub
        font-weight normal
        font-size 24px

.rl-price_1way
    padding-top 40px

.rl-price-item
    font-size 25px
    letter-spacing -.025em
    line-height 1

.rl-buy-button
    color #fff
    font-size 18px
    font-weight bold
    padding 6px 18px 5px
    box-sizing border-box
    text-align center
    border-radius 2px
    background #89c13a
    display block
    position absolute
    bottom 10px
    right 10px
    width 160px
    transition background-color 0.3s linear

.rl-buy-button:hover
    text-decoration none
    background #95cd47

.rl-airline
    display inline-block
    vertical-align middle
    margin 0 20px 0 0
    font-size 15px
    line-height 25px
    font-weight bold

    img
