.results-container
    font-family $ProximaNova
    color #333
    width 960px
    margin 0 auto

.results-container:after
    clearfix()

.results-aside
    width 246px
    float right
    background #fff

.ra-block
    border-bottom 1px solid #dedede

.ra-header
    clearfix();
    padding 12px 14px

.ra-title
    float left
    font-weight bold
    font-size 14px

.ra-header-link
    font-size 11px
    color #a9a9a9
    float right
    margin-top 3px

.ra-content
    padding 0 14px

.ra-trackbar
    margin-top 6px !important

.ra-fly-type-list
    padding 0 0 7px

.ra-fly-type-list li
    clearfix()
    font-size 13px
    letter-spacing -.025em
    padding 0 0 8px

.ra-fly-type-list input
    display none

.ra-fly-type
    float left

.ra-checkbox
    width 15px
    height 15px
    background #fff
    border 2px solid #89c13a
    float left
    border-radius 5px
    margin -2px 10px 0 0

.ra-fly-type-list input:checked + .ra-checkbox
    background url(../img/i.png) no-repeat 2px -590px #89c13a

.ra-fly-price
    float right

.ra-amount-time
    font-size 13px
    letter-spacing -.025em
    padding 0 0 18px

.ra-amount-price
    text-align center
    font-size 13px
    letter-spacing -.025em
    padding 0 0 14px
    margin -15px 0 0

.ui-slider
    height 4px
    background #d0d0d0
    margin 0 0 22px

.ui-slider .ui-slider-handle
    background url(../img/i.png) no-repeat 0 -639px
    width 15px !important
    height 22px !important
    top -12px

.ui-slider-horizontal .ui-slider-handle
    margin-left -7px

.results-list
    float left
    width 704px

.rl-header
    background #fff
    clearfix()
    font-size 14px
    border-bottom 1px solid #c8c8c8
    padding 0 0 0 12px
    margin 0 0 9px
    letter-spacing -.025em

.rl-flights-number
    float left
    font-weight normal
    padding 12px 0

.rl-filter
    float right
    position relative

.rl-filter-current
    cursor pointer
    background url(../img/i.png) no-repeat right 12px top -176px
    padding 12px 34px 12px 12px

.rl-filter-menu
    position absolute
    right 0
    background #fff
    list-style none
    box-shadow: 0 8px 8px rgba(0,0,0,.4)
    padding 0 0 5px

.rl-filter-menu li
    text-align right
    padding 6px 34px 6px 12px
    min-width 140px
    cursor pointer

.rl-filter-menu li:hover
    background #eee

.rl-item
    background #fff
    border-bottom 1px solid #ccc
    margin 0 0 9px

.rl-item-header
    clearfix()
    border-bottom 1px solid #ebebeb
    padding 7px 14px
    display block

.rl-price
    float left
    font-size 12px
    font-weight bold
    margin 5px 12px 0 0

.rl-price-item
    font-size 25px
    letter-spacing -.025em
    line-height 1

.rl-buy-button
    color #fff
    font-size 16px
    font-weight bold
    padding 4px 18px 5px
    background #89c13a
    float left

.rl-buy-button:hover
    text-decoration none

.rl-airline
    float right
    font-size 15px
    margin 6px 0 0

.rl-footer
    clearfix()
    font-size 13px
    position relative

.rl-more-info
    float left
    padding 15px 18px 14px 14px
    color #5f9c0a

.rl-more-info:hover
    text-decoration none

.rl-more-info span
    border-bottom 1px dashed #5f9c0a

.rl-tickets-number
    float right
    padding 15px 14px 14px
    color #eba600

.rl-class
    position absolute
    left 330px
    color #8b8b8b
    top 0
    padding 15px 0 14px

.rl-content
    padding 14px 14px 11px
    border-bottom 1px solid #ebebeb
    clearfix()
    font-size 13px

.rl-airline-logo
    float left
    width 35px
    height 35px
    margin 0 20px 0 0

.rl-fly
    float left
    width 138px
    padding 0 10px 0 0

.rl-fly-time
    font-size 15px
    font-weight bold
    margin-right 4px

.rl-airport
    font-size 15px

.rl-fly-date
    color #8b8b8b
    line-height 1

.rl-travel-time
    float left
    width 80px
    margin 3px 32px 0 0

.rl-travel-time
    width 70px
    position relative
    border-bottom 1px solid #b2b2b2
    text-align center
    font-size 13px
    color #8b8b8b
    padding 0 0 0 10px

.rl-travel-time:after
.rlf-fly-arrow:after
    content ''
    display block
    width 10px
    height 5px
    background url(../img/i.png) no-repeat 0 -684px
    position absolute
    right -10px
    bottom -3px

.rl-fly-type
    float right
    width 204px

.rl-fly-transplant
    font-size 15px

.rl-fly-transplant-time
    color #8b8b8b
    line-height 1

.rl-item-opened
    box-shadow 0 0 13px rgba(0,0,0,.3)

.rl-item-opened .rl-footer
    background #ebebeb

.rl-item-opened .rl-more-info
.rl-fullinfo .rl-footer
    background #fff

.rlf-fly
    padding 0 0 0 41px
    margin 25px 14px 6px
    background url(../img/i.png) no-repeat 0 -718px
    font-size 13px

.rlf-city
    font-size 15px
    font-weight bold

.rlf-fly-arrow
    margin 0 13px 0 3px
    height 0
    width 19px
    border-bottom 1px solid #b2b2b2
    display inline-block
    vertical-align middle
    position relative

.rlf-full-travel-time
    font-size 13px

.rlf-part
    clearfix()
    border-bottom 1px solid #ebebeb
    padding 16px 14px 18px
    line-height 1.2

.rlf-column1
    float left
    width 130px
    margin-right 58px

.rlf-airport
    font-size 15px
    font-weight bold

.rlf-travel-time
    font-size 13px

.rlf-colum2
    float left
    position relative

.rlf-airline-logo
    position absolute
    width 35px
    height 35px
    display block
    left -50px

.rlf-flight-info
    padding 0 0 13px

.rlf-flight-info:last-child
    padding 0

.rlf-flight-time
    font-weight bold
    font-size 15px

.rlf-flight-city
    font-size 13px

.rfl-column3
    float right
    width 205px
    font-size 13px

.rlf-aviacompany
    padding 0 0 14px

.rlf-aviacompany:last-child
    padding 0

.rlf-colum4
    float left
    width 470px
    font-size 13px

.rlf-transfer
    color #c30000

.rlf-back
    border-top 2px solid #ebebeb

.rlf-back .rlf-fly
    background-position 0 -918px

.ui-tooltip
    background #494949
    font-size 14px
    letter-spacing -.025em

.ui-tooltip:before
    border-color: transparent transparent #494949 transparent









