.rl-left .rl-fly
    width 155px

.rl-fly-time
    font-size 15px
    font-weight bold
    margin-right 4px

.rl-airport
    font-size 15px

.rl-fly-date
    color #8b8b8b
    line-height 1

.rl-travel-time
    float left
    width 115px
