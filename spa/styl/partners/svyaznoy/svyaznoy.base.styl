.partner-svyaznoy{
    .pic-cont{
        height: 0px;
        width: 0px;
        background: transparent;
    }
    .search-autocomplete{
        width 300px
        margin-top 0
        padding 0
        border 1px solid $dark-blue
        
        .search-autocomplete-block{
            &:hover,
            &.selected{
                background-color: $light-orange;
            }
        }
    }
    .offers-container{
        padding-top 13px
        
        .landing-title{
            display none
        }
    }
}