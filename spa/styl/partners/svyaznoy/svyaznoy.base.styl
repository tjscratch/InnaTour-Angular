//VARS
$bg-color = #f5f8fa
$white = #fff
$light-blue = #eef1f3
$gray = #757575
$light-gray = #f7f7f7
$light-blue2 = #afc0cd
$dark-blue = #777893
$orange = #ffb42f
$light-orange = #fff0d5

$border-radius ?= 10px

@import "../svyaznoy/colors.styl"
@import "../svyaznoy/index.styl"
@import "../full_wl/controls.styl"
@import "../full_wl/buttons.styl"
