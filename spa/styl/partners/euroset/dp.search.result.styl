.partner-wl {
    .results-body{
        //background $res-bg

        .big-map {
            top 204px
        }
    }
    .b-switch-filters {
        //top: 79px;
        top 56px
    }

    .b-dp-services__list-item-icon {
        color $green
    }
    .wl-new-search-container {
        z-index: 100;
        display: block;
        position: absolute;
        top: 0px;
        width 100%
        //left: 50%;
        //margin-left -480px
        //width: 960px;
        height: 49px;
        //background-image url("/spa/img/partners/euroset/head.jpg")
        background-color: #F0F1F5;

        .wl-center-cont {
            margin: 0 auto;
            width: 960px;
            height 49px
            position: relative;
            background-color: #F0F1F5;
        }

        .wl-container {
            color: #333;
        }

        .wl-text {
            position: absolute;
            display: block;
            left: 15px;
            top: 13px;
        }
    }

    .overlay {
        background: rgba(235, 238, 242, 0.8);
    }

    .recommended-pair-component{
        //background: $rec-bg;
    }

    .recommend-bundle-container {
        position: absolute;
        top: 0px !important;
        //left 50%
        width 100%
        //margin-left -480px
        //background: $rec-bg;
    }

    .recommend-bundle-background {
        //height 105px
        //background: $rec-bg;

        &:after{
            background none
        }
    }

    .recommend-bundle-container.recommend-bundle-container_short {
        top: 0px;
    }

    .result-buy{
        .button {
            background-color $green
        }
    }

    .results-container {
        padding: 291px 0 0;

        &.results-container__short {
            padding-top: 153px;
        }
    }

    .recommend-bundle {
        //background: $rec-bg;
    }

    .recommend-bundle-title {
        background-color: transparent;
        //line-height 23px
        line-height 29px
        padding: 10px 20px;
        margin-bottom: 0px;
        font-weight: normal;
        font-size 23px
    }

    .recommend-bundle-wrap {
        padding: 0px 0 0 0;

        &.recommend-bundle-wrap_small {
            padding: 50px 0 10px;
        }
    }

    .recommend-bundle-wrap .w-expand-collapse-link.w-expand-collapse-link_up {
        right 0
        top: 65px;
        //background-color: #fff;
        padding: 5px 14px 7px;
        text-decoration: none;
        border-radius 0
    }

    .search__list_sort {
        position relative
        box-shadow none

        label {
            padding 7px 15px 5px
        }

        .current {
            font-weight bold

            .fa-check {
                display none
            }
        }
    }

    .recommend-bundle-wrap .expand-collapse-link {
        color: $white;
        a {
            color: $white;
        }

        &.w-expand-collapse-link_down {
            color $white;
            a {
                color #fff;
            }
        }
    }

    //.icon-expand /*, .icon-collapse*/
    //{
    //    background-image: url("/spa/img/expand-collapse-gray.png");
    //}

    .ra-fly-type-list input:checked + .ra-checkbox {
        background-color $green
        border-color $green
        background-image none

        &:before {
            content ''
            position absolute
            background url("/spa/img/i.png") no-repeat scroll 2px -590px
            left 0
            right 0
            top 0
            bottom 0
            margin auto
        }
    }

    .ra-checkbox {
        position relative
        background-color $checkbox-bg
        border-color $checkbox-border
        border-radius $border-radius-3
    }
    .b-tickets__footer > .b-tickets__icons {
        display: none;
    }

    .filters__container {
        background-color #ffffff
    }

    .filter__range {
        input {
            &:focus {
                border-color $blue
            }
        }
    }

    .filters__body .filters__link_selected {
        color: #61a8e1;
    }

    .filters__baloon-radio {
        .btn-group {
            .btn {
                width 50%
                height 25px
                background transparent
                border-color #777893

                &.btn-green {
                    color $btn-color
                    background-color $btn-bg
                    text-indent 0
                    font-size 13px
                    text-transform inherit
                }
            }
        }
    }

    .search__list .current:before {
        arrow-color-wl();
    }

    .result-small {
        .corner-price {
            padding-right 20px
        }
    }

    .ui-slider {
        background-color $slider-color-2 !important
    }

    .ui-slider-range {
        background: $slider-color !important;
    }

    .filters__body {
        .filters__baloon {
            border-radius $border-radius-2
        }
    }
    .logo_inna, .icon-menu{
        display: none;
    }
    .b-search-form_padding-16px {
        padding: 16px;
    }

}