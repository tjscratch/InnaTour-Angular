$btn-bg-orange = $orange
$bg-color = $white;
$autocomplete-border-color = $blue;
$autocomplete-selected-bg = #e4e4dd;
$people-selector-btn-bg = $white;
$people-selector-btn-bg-selected = $orange;
$people-selector-btn-color-selected = $dark;
$class-selector = $orange;

$border-color = $blue
$btn-color = #fff;
