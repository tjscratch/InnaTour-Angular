$error-tooltip-color = #fff;
$form-input-error-border-color = #fb8d8c;
$form-input-error-bg = #fee8e8;

// filters
$checkbox-border = $green;
$checkbox-bg = #fff;
$checkbox-border-checked = $green;
$checkbox-bg-checked = $green;
$checkbox-color-checked = $white;
$slider-color = $orange;
$slider-color-2 = #a3b4c0;

