.partner-wl {
    $backColor = #005376;
    $hoverBackColor = #f7941e;
    $disabledBackColor = #84a7bb;

    $dateNavImg = url("data:image/png;base64,");
    .dateNav {
        position: absolute;
        width: 21px;
        height: 20px;
        cursor: pointer;
        top: 7px;
        background-image: $dateNavImg;
        background-position: left -26px;
        background-repeat: no-repeat;
    }

    $backIconLocationUrl = url("data:image/png;base64,");
    inputIconLocation() {
        background: 12px 8px no-repeat #fff;
        background-image: $backIconLocationUrl;

    }

    .b-form-container {
        max-width: 1000px;
        margin: 0 auto;
        background: $backColor;

        ::-webkit-scrollbar-track {
            background: #576778;
        }
        ::-webkit-scrollbar-thumb {
            background: #173c56;
        }

        //автокомплит
        .b-inna-search-form-field .dropdown-menu li {
            color $backColor;

            &.active {
                background: $hoverBackColor;
                color #fff;

                .i-country, .b-location-item-iata {
                    color #fff;
                }
            }

        }

        .b-inna-search-widget {
            position: relative;
        }

        .b-inna-search__nav {
            display: none;
        }

        .b-inna-search-form {
            padding: 12px 0 12px 12px;
            background-color: transparent;
        }

        .inna-tooltip {
            top: 115%;
            z-index: 10;
        }

        .b-inna-search-form-field-input {
            border-radius: 0;
            height: 34px;

            &:focus {
                box-shadow: none;
                border-color: transparent;
            }
        }

        .b-form-direction {
            position: relative;
            display: inline-block;
            margin: 13px 0 1px 12px;
            line-height: normal;

            &__selector {
                display: inline-block;
                margin-right: 9px;
                border-bottom: 1px dotted #fff;
                color: #fff;
                cursor: pointer;
                font-size: 10px;
                vertical-align: middle;
                text-transform: uppercase;

                &.active {
                    padding: 3px 8px 4px 7px;
                    border-bottom: none;
                    background: $hoverBackColor;
                    line-height: 14px;
                }
            }

        }

        .b-form-date-nav {
            position absolute;
            top: 32px;
            /*top: 47px;*/
            left: 550px;
            z-index: 1;

            .dateNav {
                display inline-block;

                &.disabled {
                    cursor default;
                    background-position: left 0px;

                    &:hover {
                        background-position: left 0px;
                    }
                }

                &:hover {
                    background-position: left bottom;
                }

                &.next {
                    background-position: right -26px;

                    &.disabled {
                        background-position: right 0px;

                        &:hover {
                            background-position: right 0px;
                        }
                    }

                    &:hover {
                        background-position: right bottom;
                    }
                }

                &:nth-child(1) {
                    margin-left: 3px;
                }
                &:nth-child(2) {
                    margin-left: 183px;
                }
                &:nth-child(3) {
                    margin-left: 228px;
                }
                &:nth-child(4) {
                    margin-left: 408px;
                }
            }
        }
        .b-form-labels {
            position: relative;
            display: inline-block;
            height: 15px;
            top: 6px;
            left: 12px;
            color: #fff;
            font-size: 10px;
            text-transform: uppercase;
            line-height: 12px;

            span {
                display: inline-block;

                &:nth-child(2) {
                    margin-left: 231px;
                }
                &:nth-child(3) {
                    margin-left: 228px;
                }
                &:nth-child(4) {
                    margin-left: 155px;
                }
            }
        }
        .b-form-passengers {
            position: relative;
            display: inline-block;
            margin: 3px 0 10px 11px;
        }

        .b-form-passengers__select-block {
            display: inline-block;
            position: relative;
            float: left;
            margin: 0 0 0 0;

            &.childs {
                margin: 0 15px 0 78px;
            }
        }

        .b-form-passengers__label {
            display: inline-block;
            float: left;
            margin: 0 9px 0 0;
            color: #fff;
            font-size: 10px;
            text-transform: uppercase;
            line-height: 12px;
        }

        .infants .b-form-passengers__nums {
            margin-left: 22px;
        }

        .b-form-passengers__nums {
            position: relative;
            display: inline-block;
            margin: 2px 0 0 0;
            padding: 0;

            span {
                display: inline-block;
                position: relative;
                float: left;
                width: 21px;
                height: 21px;
                line-height: 21px;
                font-weight: bold;
                font-size: 12px;
                color: #456f9a;
                text-align: center;
                cursor: pointer;
                vertical-align: top;

                &:hover {
                    color: #fff;
                    background: $hoverBackColor;
                }

                &.active {
                    color: #fff;
                    background: $hoverBackColor;
                }
            }

            .plus {
                position: absolute;
                top: 0;
                right: 1px;
            }
        }

        .b-form-passengers__child-ages {
            position: relative;
            display: inline-block;
            margin: -5px 0 0 0;
            padding: 0;
        }

        .b-form-passengers__age {
            position: relative;
            display: inline-block;
            float: left;
            margin: 0 5px;
            width: 64px;
            height: 36px;
            padding: 8px 0 0 14px;
            background-color: #fff;
            cursor: pointer;

            &.disabled {
                background-color: $disabledBackColor;
                cursor default;
            }
        }

        .b-form-passengers__age-dropdown-wrap {

        }
        .b-form-passengers__age-dropdown {
            position: absolute;
            top: 100%;
            left: 0;
            /*width: 100%;*/
            //скролбар - внутри
            width: 68px; //скроллбар - наружу
            height: 182px;
            overflow-y: scroll;
            background-color: #fff;
            text-align: center;

            li {
                display: block;
                padding: 3px;

                &:hover {
                    background-color: $hoverBackColor;
                }
            }
        }

        .b-form-passengers__b-tarif {
            display: inline-block;
            position: relative;
            margin: 1px 0 0 20px;

            span {
                display: inline-block;
                margin-right: 5px;
                border-bottom: 1px dotted #fff;
                color: #fff;
                cursor: pointer;
                font-size: 10px;
                vertical-align: middle;
                text-transform: uppercase;
                line-height: normal;

                &.active {
                    padding: 3px 8px 4px 8px;
                    border-bottom: none;
                    background: #f7941e;
                    line-height: 14px;
                }
            }
        }

        .b-form-submit {
            display: inline-block;
            position: absolute;
            right: 12px;
            bottom: 14px;
            margin: 0;

            .button {
                background: url('/spa/img/partners/biletix/btns_find.png') left top no-repeat;
                width: 109px;
                height: 36px;
                position: relative;
                right: 0;
                top: 0;
                padding: 0;
                border: none;

                display: inline-block;
                vertical-align: middle;
                zoom: 1;
                cursor: pointer;
                font-size: 0;

                &.disabled {
                    background-position-x: -437px;
                }
            }
        }

        //наша форма
        .b-inna-col-xs-3 {
            &.disabled {
                opacity: 0.6;
            }
        }

        //куда откуда
        .b-inna-search-form-field.__typeahead {
            margin: 0 10px 0 0;

            .b-inna-search-form-field-input {
                width: 257px;
                padding: 5px 0 8px 29px;
                inputIconLocation();
                color: #084e76
                font-weight: bold;
                font-size: 14px;
                line-height: 17px;
            }
        }

        //даты
        .b-inna-search-form-field-input.b-inna-datapicker {
            width: 215px;
            padding: 0px 0px 2px 32px;
            background: #fff;
            color: #084e76
            font-weight: bold;
            font-size: 14px;
            line-height: 17px;
            /*font-size: 11px;*/
            /*line-height: 14px;*/
            /*text-transform: uppercase;*/

            &.from_date {
                margin: 0 10px 0 0;
            }

        }

        .inna-dropdown, .b-inna-search-form-field.__action {
            display: none;
        }

        .i {
            width: 14px;
            height: 16px;
            display: inline-block;
            position: absolute;
            cursor: pointer;
            margin-top: -19px;
            margin-left: 20px;
            background: url("data:image/png;base64,") left top no-repeat;
        }

        .arrow-down {
            display: inline-block;
            position: absolute;
            top: 38%;
            right: 8px;
            width: 12px;
            height: 8px;
            background-image: url("data:image/png;base64,");
        }

        /*.i-location {*/
        /*background-image: $backIconLocationUrl;*/
        /*}*/

    }

    $dpBgColor = #dae5e9;
    $dpColor = #2f5474;

    $dpSelectedBgColor = #8dc63f;
    $dpSelectedColor = #fff;

    $dpHoverBgColor = #00ace4;
    $dpHoverColor = #fff;

    $dpBorderColor = #e9ecee;

    $dpHeadColor = #a9b3c0;

    //date picker
    .datepicker .table-condensed > tbody > tr > td.range, .datepicker table > tbody > tr > td.range {
        background-color: $dpBgColor;
        color: $dpColor;

        &:hover {
            background-color $dpHoverBgColor;
            color $dpHoverColor;
        }
    }

    .datepicker .table-condensed > tbody > tr > td.day, .datepicker table > tbody > tr > td.day {
        color: $dpColor;

        &:hover {
            background-color $dpHoverBgColor;
            color $dpHoverColor;
        }

        &:nth-of-type(6),
        &:nth-of-type(7){
            &:hover {
                color #ec008c;
            }
        }
    }

    .datepicker .table-condensed > tbody > tr > td.disabled, .datepicker table > tbody > tr > td.disabled {
        opacity: 0.5;
        background-color: transparent !important;
        color: $dpColor;
        cursor default;

        &:hover {
            background-color transparent !important;
            color $dpColor;
            cursor default;
        }
    }

    .datepicker .table-condensed > tbody > tr > td.selected, .datepicker table > tbody > tr > td.selected {
        vertical-align inherit;
    }

    .datepicker .table-condensed > tbody > tr > td.selected.from_date, .datepicker table > tbody > tr > td.selected.from_date {
        vertical-align top;
        background-color: $dpSelectedBgColor;
        color: $dpSelectedColor;

        &:hover {
            background-color $dpHoverBgColor;
            color $dpHoverColor;
        }
    }

    .datepicker .table-condensed > tbody > tr > td.selected.to_date, .datepicker table > tbody > tr > td.selected.to_date {
        vertical-align top;
        background-color: $hoverBackColor;
        color: $dpSelectedColor;

        &:hover {
            background-color $dpHoverBgColor;
            color $dpHoverColor;
        }
    }

    .datepicker .datepicker-switch {
        color #06538c;
        font-weight bold;
    }

    .datepicker table th.dow {
        color #a9b3c0;
    }

    .datepicker-days .table-condensed {
        &:before {
            content '';
            position absolute;
            top: 67px;
            left: 8px;
            width: 232px;
            border-bottom 1px solid #c2d3db;
        }
    }

    .datepicker table th.prev, .datepicker table th.next {
        &:hover {
            background-color #f2f6f7;
        }
    }

    //сб вс
    .datepicker .datepicker-days > table > thead > tr:nth-child(2) > th:nth-child(6),
    .datepicker .datepicker-days > table > thead > tr:nth-child(2) > th:nth-child(7) {
        color #ff85cd;
    }

    .datepicker .table-condensed > tbody > tr > td:nth-of-type(6),
    .datepicker table > tbody > tr > td:nth-of-type(6),
    .datepicker .table-condensed > tbody > tr > td:nth-of-type(7),
    .datepicker table > tbody > tr > td:nth-of-type(7) {
        color #ec008c;

        &.disabled {
            color #ec008c;
        }
    }

    /*.datepicker .table-condensed > tbody > tr > td.today, .datepicker table > tbody > tr > td.today {*/
    /*border 2px solid $dpBorderColor;*/

    /*&:hover {*/
    /*border 2px solid $dpHoverBgColor;*/
    /*}*/
    /*}*/
}