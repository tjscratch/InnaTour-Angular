@import "../full_wl/full_wl.base.styl"

.partner-biletix {
    $landPadding = 50px;
    
    .b-dp-services__list-item-icon{
        color #ec008c
    }
        
    .light-theme, .lighten-theme {
        background none;
    }

    .wrpr {
        padding-bottom 0px !important;
        background #eaeff3;
    }

    .overlay.overlay-baloon {
        background none;
    }

    .overlay {
        background: rgba(0, 0, 0, 0.4);
    }

    /*отключаем фон на главной*/
    .b-main-page {
        &.wrpr {
            background transparent;
        }
    }

    .layout.partner {
        top: $landPadding;
        margin-bottom: $landPadding;
        padding: 48px 0 0;
    }

    .offers-container {
        padding-top 0;
    }

    .offers-list_new-landing .landing-title {
        padding-bottom 130px;
    }

    .landing-title {
        text-align: center;
        color: #fff;
        font-size: 36px;
        line-height: 42px;
    }

    .b-tour-form {
        padding-left: 100px;
    }

    .pic-cont {
        height: 0px;
        width: 0px;
        background: transparent;
    }
}