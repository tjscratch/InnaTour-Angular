.partner-wl {
    .light-theme {
        background-color #fff;
    }

    /*header*/
    .header-info {
        padding 10px 0;
        background-color #fff;
    }

    .header-info-container {
        line-height inherit;

        color: #0069aa;

        font-family: Roboto;
        font-size: 16px;
        font-style: normal;
        font-variant: normal;
        font-weight: 300;
    }

    .b-head-text-container {
        position absolute;
        top: 18px;
        left: 100px;

        div {
            display inline-block;
        }

        .b-select-city {
            display: inline-block;
            position relative;
            cursor: pointer;

            .arrow {
                display: inline-block;
                width: 14px;
                height: 14px;
                margin-left: 6px;
                margin-bottom: -2px;
                background: url('data:image/png;base64,');
            }
        }

        .b-office {
            position relative;
            margin-left 40px;
            border-bottom: 1px dotted #0069aa;
            cursor pointer;

            &.open {
                color: #f7941c;
                border-bottom: 1px dotted #f7941c;

                .b-office__dropdown {
                    display block;
                }
            }

            &:hover {
                color: #f7941c;
                border-bottom: 1px dotted #f7941c;
            }

            &__dropdown {
                display none;
                position: absolute;
                z-index: 9999;
                width: 294px;
                padding: 20px 13px 0;
                left: -15px;
                top: 125%;
                height: 190px;
                border-left: 1px solid rgba(242, 242, 242, 0.8);
                border-right: 1px solid rgba(242, 242, 242, 0.8);
                background: #fff;
                overflow-x: hidden;
                overflow-y: auto;
                box-shadow: 0 3px 3px rgba(0, 0, 0, 0.15);
            }

            &__dropdown-item {
                font-size: 16px;
                font-weight: 300;
                color: #333;
                line-height: 24px;
                border-top: 1px solid #ebebeb;
                padding: 15px 0;
                width: 94%;

                &:first-child {
                    border-top: 0;
                    padding-top: 0;
                }

                a {
                    text-decoration: none;
                    color: #0069aa;
                    font-weight: 400;
                    display: block;

                    &:hover {
                        color: #f7941c;
                    }
                }
            }
        }

        .b-phone {
            position relative;
            margin-left 40px;
            color: #323232;

            .txt {
                position absolute;
                top: 1px;
                width: 80px;
                color: #8f8f8f;

                span {
                    display: inline-block;
                    font: 100 11px/12px 'Roboto';
                }

                .i {
                    color: #323232;
                    font: 700 10px / 12px 'Roboto';
                    text-transform: uppercase;
                }
            }

            .num {
                margin-left 80px;
                font: 500 24px / 24px 'Roboto';
                color: #333;
            }
        }

        .b-nav-menu {
            margin 0 0 -4px 20px;

            li {
                float: left;
                margin: 0 0 0 20px;

                a {
                    font: 700 12px / 18px 'Roboto';
                    color: #0069aa;
                    text-decoration: none;
                    text-transform: uppercase;

                    &:hover {
                        color: #f7941c;
                    }
                }
            }
        }
    }

    .icon-logo {
        width: 77px;
        height: 65px;
        background: url(/spa/img/partners/sputnik/logo.png) no-repeat 0 0;
    }

    /*footer*/
    .footer {
        background-color #eeeeee;
    }
}
