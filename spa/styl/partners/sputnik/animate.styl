.partner-wl {
    @keyframes progress {
        0% {
            background-position: 50% 0;
        }
        25% {
            background-position: 100% 0;
        }
        50% {
            background-position: 50% 0;
        }
        75% {
            background-position: 0% 0;
        }
        100% {
            background-position: 50% 0;
        }
    }

    .panorama {
        -webkit-animation: progress 40s ease infinite;
        animation: progress 40s ease infinite;
        transition: all 155.8s ease-out;
    }
}