.layout.partner {
    top: 390px; 
    margin-bottom: 390px;
}

.partner-form-wrap{
  margin: 0 auto;
  max-width: 974px;
}

.b-tour-form
  position absolute
  top 50px
  /*left 300px*/
  padding-left 120px/*отступ*/

  &-wrapper
    background #fff
    padding 0 19px 21px
    width 401px

  .main-label
    color-bg-lbl-wl()
    font-size 27px
    font-weight bold
    color #fff
    display inline-block
    line-height 44px
    padding 0 18px

  .b-search-field
    border 1px solid #bfbfc1
    display inline-block
    height 49px
    margin 14px 0 4px
    vertical-align top

    .search-people{
      background-image: none;
    }

    .search__list{
      margin-top: 2px;
    }

    .search-form-dialog-row .button-group .button.active{
      color-bg-btn-wl();
    }

    &__long
      width 399px

    &__half
      width 189px

    &__class
      width 119px
      margin-left 20px
      margin-right 10px

    &__calendar,
    &__list
      position relative

      &:after
        background url("/spa/img/i.png") no-repeat
        content ''
        display block
        position absolute
        top 50%;
        right 9px

    &__list
      &:after
        background-position -45px -194px
        width 11px
        height 6px
        margin-top -3px

    &__calendar
      &:after
        background-position -39px -1830px
        width 17px
        height 16px
        margin-top -8px

      background-position 0 -171px

    &-hor
      text-align justify
      font-size 0

      &:after
        width 100%
        content: ''
        display inline-block
        height 0

    &-value,
    input
      box-sizing border-box
      border 0
      height 100%
      width 100%
      padding 0 10px
      font-size 16px

    &-value
      line-height 49px

      .description
        color #868685

      .iata
        position absolute
        color #868685
        right 30px

  .b-search-button
    color-bg-btn-wl()
    display inline-block
    height 49px
    width 58px
    margin-top 14px
    cursor: pointer;

    &:after
      @extend .icon-sprite-search;
      content: '';
      display: block;
      position: absolute;
      /*top: 50%;*/
      /*right: 14px;*/
      /*margin-top: -15px;*/
      right: 30px;
      margin-top: 12px;
      z-index: 1;

.dtpk-checkbox
  border-color #3fa9f5
  border-radius 0

.dtpk-checkbox-label input:checked + .dtpk-checkbox
  background-color #3fa9f5

.ui-datepicker td a.ui-state-active,
.ui-datepicker td a.ui-state-hover
  background-color #3fa9f5

.search-form-human-dialog{
  width: 100%;
}

.search-date-block__date{
  font-size: 16px;
}

.search-date-block__date,
.search-people,
.search-form-item-current{
  height: 49px;
  line-height: 49px;
}

.datepickerSelected-start a
.datepickerSelected-end a
  color-datepicker-background-wl()

.datepickerSelected a{
  color-datepicker-bg();
}

.datePicker-today a{
  color-border-wl()
}

.datepickerDays a
  &:hover
    color-datepicker-background-wl()

.datepickerGoPrev a
.datepickerGoNext a
  &:hover
    color-datepicker-background-wl()

.search-autocomplete{
  width: 401px;
  padding: 0;
  margin-left: -1px;
}

.b-tour-form{
  .b-search-field__long{
    &.b-search-field__from{
      z-index: 31;
    }
    &.b-search-field__to{
      z-index: 30;
    }

    .ui-tooltip{
      position: absolute;
      left: 410px !important;
      top: 5px !important;
      margin-left: 0 !important;

      &:before{
        display none
      }

      .ui-tooltip-content{
        white-space nowrap;
      }
    }
  }
}

.datepicker-field{
  position: relative;

  .ui-tooltip{
    position absolute;
    left 410px
    bottom 10px

    &:before{
      display none
    }

    .ui-tooltip-content{
      white-space nowrap;
    }
  }
}